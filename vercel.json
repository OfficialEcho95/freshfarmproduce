{
    "version": 2,
    
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],

    { "rewrite": [{
        "source": "/.(*)", 
        "destination": "/"
    }]},

    "routes": [
        {
            "src": "/commodityUploads/(.*)",
            "dest": "/commodityUploads/$1"
        },
        {
            "src": "/js/html/(.*)",
            "dest": "/frontend/js/html/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "/frontend/js/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        },
        {
            "src": "/",
            "dest": "/frontend/index.html"
        }
    ]
}