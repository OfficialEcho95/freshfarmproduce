{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server.js"
        },
        {
            "src": "/commodityUploads/(.*)",
            "dest": "/commodityUploads/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "/frontend/js/html/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/index.html"
        },
        {
            "src": "/favicon.ico",
            "dest": "/favicon.ico"
        }
    ]
}